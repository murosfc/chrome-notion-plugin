{
  "geminiApiKey": "PASTE_YOUR_GEMINI_API_KEY_HERE",
  "projectPath": "/path/to/your/project",
  "settings": {
    "serverPort": 3000,
    "branchPrefix": "auto",
    "defaultBranchType": "feat",
    "maxBranchNameLength": 60,
    "generateBranchDescription": false,
    "defaultBaseBranch": ""
  },
  "advanced": {
    "gitTimeout": 30000,
    "apiTimeout": 10000,
    "debugMode": false,
    "autoCheckout": true,      // true = automatic checkout (default) | false = create only
    "backupBranch": true
  },
  "examples": {
    "geminiApiKey_help": "Get your key at: https://makersuite.google.com/app/apikey",
    "projectPath_examples": [
      "Windows: C:\\projects\\my-project",
      "Linux/Mac: /home/user/projects/my-project",
      "Mac: /Users/user/projects/my-project"
    ],
    "branchPrefix_options": [
      "auto - Let Gemini decide (feat/, fix/, etc.)",
      "custom - Use a custom prefix",
      "none - No prefix"
    ],
    "defaultBaseBranch_help": "Default base branch for creating new branches (e.g., main, develop). If empty, uses current branch.",
    "defaultBaseBranch_examples": [
      "main - Most common default branch",
      "develop - Development branch in GitFlow",
      "master - Legacy default branch",
      "'' (empty) - Use current branch as base"
    ]
  }
}
